{
  "cleanupPeriodDays": 90,
  "includeCoAuthoredBy": false,
  "permissions": {
    "allow": [
      "Bash(*)",
      "Read(*)",
      "Write(*)",
      "Edit(*)",
      "MultiEdit(*)",
      "Glob(*)",
      "Grep(*)",
      "LS(*)",
      "mcp__context7__get-library-docs",
      "mcp__context7__resolve-library-id",
      "NotebookEdit(*)",
      "NotebookRead(*)",
      "WebFetch(*)",
      "WebSearch(*)",
      "TodoRead",
      "TodoWrite",
      "Agent(*)",
      "playwright:*"
    ],
    "deny": []
  },
  "env": {
    "MAX_MCP_OUTPUT_TOKENS": "50000",
    "MCP_TOOL_TIMEOUT": "30000"
  }
}

